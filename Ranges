fun main()
{
    for(i in 1..5 )           //print range
    print("$i,")
    
    print("     ")
    
    for(i in 10 downTo 1)         //print range till 1
    print("$i,")
    
    print("     ")
    
    for(i in 1..10 step 2)       //print range by +2
    print("$i,")
    
    print("     ")
    
    for(i in 10 downTo 1 step 2)
    print("$i,")
    
      print("     ")
      
    for(i in 'a'..'z')           //print range
    print("$i,")
    
}
